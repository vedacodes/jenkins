pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('this') {
            steps {
                echo 'Hello World'
            }
        }
        stage('is') {
            steps {
                echo 'Hello World'
            }
        }
        stage('pipeline') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
